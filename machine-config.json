{
  "env": {
    "PRIMARY_REGION": "nrt"
  },
  "init": {},
  "guest": {
    "cpu_kind": "shared",
    "cpus": 1,
    "memory_mb": 1024
  },
  "metadata": {
    "fly-managed-postgres": "true",
    "fly_flyctl_version": "0.3.190",
    "fly_platform_version": "v2",
    "managed-by-fly-deploy": "true"
  },
  "mounts": [
    {
      "volume": "vol_rn853e5m56e9yw8r",
      "path": "/data"
    }
  ],
  "services": [
    {
      "protocol": "tcp",
      "internal_port": 5432,
      "autostart": false,
      "ports": [
        {
          "port": 5432,
          "handlers": ["pg_tls"]
        }
      ],
      "concurrency": {
        "type": "connections",
        "hard_limit": 1000,
        "soft_limit": 1000
      },
      "force_instance_key": null
    },
    {
      "protocol": "tcp",
      "internal_port": 5433,
      "autostart": false,
      "ports": [
        {
          "port": 5433,
          "handlers": ["pg_tls"]
        }
      ],
      "concurrency": {
        "type": "connections",
        "hard_limit": 1000,
        "soft_limit": 1000
      },
      "force_instance_key": null
    }
  ],
  "metrics": {
    "port": 9187,
    "path": "/metrics"
  },
  "checks": {
    "pg": {
      "port": 5500,
      "type": "http",
      "interval": "15s",
      "timeout": "10s",
      "path": "/flycheck/pg"
    },
    "role": {
      "port": 5500,
      "type": "http",
      "interval": "15s",
      "timeout": "10s",
      "path": "/flycheck/role"
    },
    "vm": {
      "port": 5500,
      "type": "http",
      "interval": "15s",
      "timeout": "10s",
      "path": "/flycheck/vm"
    }
  },
  "image": "docker-hub-mirror.fly.io/flyio/postgres-flex:17.2@sha256:f4301ae20d193ab3c3539eb9df9a8f8d3736dd331aeec1bfb2e34b539dc353c5",
  "restart": {
    "policy": "always"
  }
}